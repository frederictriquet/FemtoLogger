{
  "name": "femtologger",
  "version": "0.1.0",
  "description": "Lightweight, extensible event logger for TypeScript server apps",
  "keywords": [
    "logger",
    "logging",
    "telegram",
    "typescript",
    "events"
  ],
  "author": "Fred",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  }
}
