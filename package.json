{
  "name": "@frederictriquet/femtologger",
  "version": "0.1.1",
  "description": "Lightweight, extensible event logger for TypeScript server apps",
  "keywords": [
    "logger",
    "logging",
    "telegram",
    "typescript",
    "events"
  ],
  "author": "Fred",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/frederictriquet/FemtoLogger"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.18"
  }
}
